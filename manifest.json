{
  "manifest_version": 3,
  "name": "BackupMyListings",
  "version": "1.0",
  "description": "A Chrome extension to help organize and download files",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js"
}
}