{
  "manifest_version": 3,
  "name": "BackupMyListings",
  "version": "1.0",
  "description": "A Chrome extension to help organize and download files",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "activeTab",
    "tabs",
    "scripting",
    "downloads.shelf"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "externally_connectable":
  {
  "matches": ["https://pdfc072b6ef924743b4ea05c7a3f4dd37.bildr.com/download-files"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
